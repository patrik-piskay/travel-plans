{
  "plugins": ["node"],
  "extends": ["react-app", "plugin:node/recommended"],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "env": {
    "node": true
  },
  "rules": {
    "node/no-unpublished-import": [
      "error",
      {
        "allowModules": ["supertest", "ava"]
      }
    ]
  }
}
